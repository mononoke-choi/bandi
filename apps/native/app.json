{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4C7AED",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.bandi"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "hidden": false
    },
    "assetBundlePatterns": ["**/*"],
    "backgroundColor": "#ffffff",
    "description": "Digital therapeutics software",
    "extra": {
      "router": {
        "asyncRoutes": "development",
        "origin": "<YOUR_WEB_URL>.vercel.app"
      }
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "bundleIdentifier": "com.bandi",
      "infoPlist": {
        "RCTAsyncStorageExcludeFromBackup": true,
        "UIBackgroundModes": ["remote-notification"]
      },
      "supportsTablet": true
    },
    "name": "Bandi",
    "orientation": "portrait",
    "plugins": ["expo-notifications"],
    "primaryColor": "#ffffff",
    "privacy": "unlisted",
    "scheme": "bandi",
    "slug": "bandi",
    "splash": {
      "backgroundColor": "#4C7AED",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro"
    }
  }
}
