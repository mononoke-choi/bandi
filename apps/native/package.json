    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.4.3",
    "@tamagui/babel-plugin": "^1.43.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "dayjs": "^1.11.9",
    "eslint-config-custom": "*",
    "expo": "~49.0.3",
