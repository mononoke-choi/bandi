{
  "name": "native",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "yarn start",
    "start": "yarn debugger && APP_ENV=development expo start --ios --dev-client -c",
    "start:all": "APP_ENV=development expo start --ios --android --dev-client",
    "ios:build:local": "APP_ENV=development expo run:ios",
    "ios:build:eas": "eas build:run --platform ios",
    "android:build:local": "APP_ENV=development expo run:android",
    "android:build:eas": "eas build:run --platform android",
    "eas:resignADHOC": "eas build:resign",
    "detox:prepare": "expo prebuild --clean",
    "detox:android:debug:build": "detox build -c android.debug",
    "detox:android:debug:test": "detox test -c android.debug",
    "detox:android:release:build": "detox build -c android.release",
    "detox:android:release:test": "detox test -c android.release",
    "detox:ios:debug:build": "detox build -c ios.debug",
    "detox:ios:debug:test": "DETOX=true detox test -c ios.debug",
    "detox:ios:release:build": "detox build -c ios.release",
    "detox:ios:release:test": "DETOX=true detox test -c ios.release",
    "debugger": "unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=8081'",
    "lint": "eslint . --fix",
    "clean": "rm -rf .expo .expo-shared ios android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@config-plugins/detox": "^5.0.1",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/animations-react-native": "^1.20.7",
    "@tamagui/babel-plugin": "^1.20.7",
    "@tamagui/config": "^1.20.7",
    "@tamagui/font-inter": "^1.20.7",
    "@tamagui/lucide-icons": "^1.20.7",
    "@tamagui/themes": "^1.20.7",
    "api": "*",
    "dayjs": "^1.11.7",
    "detox": "^20.7.0",
    "eslint-config-custom": "*",
    "expo": "~48.0.13",
    "expo-clipboard": "^4.1.2",
    "expo-constants": "^14.2.1",
    "expo-dev-client": "^2.2.1",
    "expo-device": "^5.2.1",
    "expo-font": "~11.1.1",
    "expo-linear-gradient": "^12.1.2",
    "expo-linking": "~4.0.1",
    "expo-notifications": "^0.18.1",
    "expo-router": "^1.5.3",
    "expo-secure-store": "^12.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-web-browser": "^12.1.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
